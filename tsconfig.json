{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "strict": true,
        "jsx": "react",
        "sourceMap": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "lib": ["ESNext", "DOM"],
        "skipLibCheck": true,
        "baseUrl": "./",
        "paths": {
            "@/*": ["resources/js/*"]
        },
        "noImplicitAny": false,
        "allowJs": true,
        "types": ["@intlify/unplugin-vue-i18n/messages"]
    },
    "include": ["resources/js/**/*.js", "resources/js/**/*.ts", "resources/js/**/*.d.ts", "resources/js/**/*.tsx", "resources/js/**/*.vue", "resources/js/App.vue", "resources/js/app-setting.ts", "resources/js/i18n.ts", "resources/js/main.ts", "resources/js/vite-env.d.ts"],
    "references": [
        {
            "path": "./tsconfig.node.json"
        }
    ]
}
